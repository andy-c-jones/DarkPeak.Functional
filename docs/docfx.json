{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/DarkPeak.Functional/**/*.csproj",
            "src/DarkPeak.Functional.Http/**/*.csproj",
            "src/DarkPeak.Functional.AspNet/**/*.csproj",
            "src/DarkPeak.Functional.Redis/**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "api/**.yml", "api/index.md" ] },
      { "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ] }
    ],
    "resource": [
      { "files": [ "images/**" ] }
    ],
    "dest": "_site",
    "template": [ "default", "modern" ],
    "globalMetadata": {
      "_appTitle": "DarkPeak.Functional",
      "_appFooter": "DarkPeak.Functional â€” A functional programming library for .NET",
      "_enableSearch": true
    }
  }
}
